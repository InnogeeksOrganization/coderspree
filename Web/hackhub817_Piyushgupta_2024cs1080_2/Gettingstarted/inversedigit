import java.util.*;

public class Main{

public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
      int c=0;
      int sum=0;
      while(n!=0)
      {
          int d=n%10;
          c++;
          int s=(int) Math.pow(10,d-1);
          int j=s*c;
          sum=sum+j;
          n=n/10;
      }
    System.out.println(sum);      
 }
 
}